{
  "name": "js-context",
  "version": "0.9.2",
  "type": "module",
  "keywords": [
    "context",
    "ctx",
    "go"
  ],
  "description": "Context library inspired by go",
  "main": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "repository": "https://github.com/brandonryan/js-context",
  "author": "Brandon Ryan",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc && yarn run copyfiles -u1 src/*.d.ts dist/types",
    "prepublishOnly": "yarn run build && yarn run test && yarn run test-types",
    "test": "node --no-warnings --experimental-vm-modules node_modules/.bin/jest",
    "test-types": "tsd"
  },
  "files": [
    "./src/",
    "./dist/"
  ],
  "jest": {
    "transform": {}
  },
  "tsd": {
    "directory": "test"
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/modules/*.js"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "copyfiles": "^2.4.1",
    "jest": "^27.0.6",
    "tsd": "^0.18.0",
    "typescript": "^4.4.4"
  }
}
